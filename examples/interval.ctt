module interval where

import prelude

data I = zero | one | seg @ zero ~ one

fext (A B : U) (f g : A -> B) (p : (x : A) -> Id B (f x) (g x)) :
     Id (A -> B) f g = <j> (\(x : A) -> htpy x (seg{I} @ j))
  where htpy (x : A) : I -> B = split
          zero    -> f x
          one     -> g x
          seg @ i -> (p x) @ i
